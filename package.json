{
  "name": "game",
  "version": "0.0.0",
  "description": "simple game",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "webpack -p --mode production --https false",
    "server": "webpack-dev-server --mode development",
    "prepack": "npm run build",
    "build2": "npm run style && npm run lint && npm run clean && npm run compile && npm run doc",
    "clean": "if exist dist (rmdir /S /Q dist)",
    "style": "npm run prettier",
    "lint": "npm run eslint",
    "lint-fix": "npm run eslint-fix",
    "compile": "./node_modules/.bin/tsc --project ./tsconfig.json",
    "test": "jest --no-cache --config=./config/jest.config.js",
    "prettier": "./node_modules/.bin/prettier --config ./config/prettier.json --write \"@(src|test)/**/*.@(ts|js|md|html|css|json)\"",
    "eslint": "./node_modules/.bin/eslint -c ./config/.eslintrc.json --no-eslintrc src/ test/",
    "eslint-fix": "./node_modules/.bin/eslint -c ./config/.eslintrc.json --no-eslintrc --fix src/ test/",
    "api-extractor": "./node_modules/.bin/api-extractor",
    "doc": "npm run doc-extract && npm run doc-document",
    "doc-extract": "./node_modules/.bin/api-extractor run --local --verbose",
    "doc-document": "./node_modules/.bin/api-documenter markdown --input-folder ./temp --output-folder ./doc",
    "check": "./node_modules/.bin/ts-node scripts/check.ts",
    "start": "./node_modules/.bin/ts-node test/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wandyezj/game.git"
  },
  "author": "wandyezj",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/wandyezj/game/issues"
  },
  "homepage": "https://github.com/wandyezj/game#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node": "^13.12.0",
    "office-addin-cli": "^1.0.7",
    "office-addin-dev-certs": "^1.4.14",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.11.0",
    "@microsoft/api-documenter": "^7.13.28",
    "@microsoft/api-extractor": "^7.17.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
